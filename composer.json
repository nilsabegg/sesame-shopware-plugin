{
  "name": "nilsabegg/sesame-shopware-plugin",
  "license": "MIT",
  "type": "settings",
  "description": "A Shopware Plugin to integrate Sesame - an AliExpress dropshipping library.",
  "keywords": [
    "PHP",
    "Shopware",
    "AliExpress",
    "Dropshipping"
  ],
  "authors": [
    {
      "name": "Nils Abegg",
      "email": "nils.abegg@gmail.com"
    }
  ],
  "extra": {
    "installer-name": "ShopwareSesame"
  },
  "require": {
    "php": "^7.1.3",
    "nilsabegg/sesame": "dev-master",
    "shopware/shopware": "5.4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.6||~6.3",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "Shopware\\Sesame\\": "./"
    }
  }
}